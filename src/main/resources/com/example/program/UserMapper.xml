<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.program.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.example.program.entity.User">
            <id property="open_id" column="open_id" jdbcType="VARCHAR"/>
            <result property="nickName" column="nickName" jdbcType="VARCHAR"/>
            <result property="avatarUrl" column="avatarUrl" jdbcType="VARCHAR"/>
            <result property="property" column="property" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        open_id,nickName,avatarUrl,
        property
    </sql>
</mapper>
